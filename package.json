{
  "version": "0.6.0",
  "name": "orgfld",
  "description": "Folds org-mode markdown documents by heading level using * symbol",
  "repository": "https://github.com/ukdor/orgfld",
  "engines": {
    "atom": ">=1.9.0 <2.0.0"
  },
  "keywords": [
    "markdown",
    "org-mode",
    "fold",
    "folding",
    "collapse",
    "heading"
  ],
  "main": "./lib/main",
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "prettier": "^1.5.3",
    "prettier-eslint-cli": "^4.1.1"
  },
  "eslintConfig": {
    "globals": {
      "atom": false
    },
    "extends": [
      "standard"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline"
        }
      ],
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^__"
        }
      ]
    }
  }
}
